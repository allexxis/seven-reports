{
   "extends": "expo/tsconfig.base",
   "compilerOptions": {
      "strict": true,
      "paths": {
         "@src/*": ["./src/*"],
         "@components/*": ["./src/components/*"],
         "@assets/*": ["./src/assets/*"],
         "@utils/*": ["./src/utils/*"]
      }
   },
   "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
